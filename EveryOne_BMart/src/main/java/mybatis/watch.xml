<?xml version="1.0" encoding="UTF-8"?>

<!-- watch.xml -->
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="watch.Watch">
	<select id="getWatchLists" resultType="mall.model.WatchBean">
		select * from watch where id=#{id} order by wnum desc
	</select>
</mapper>